{
  "extends": ["plugin:vue/recommended"],
  "env": {
    "browser": true,
    "node": false,
    "es6": true,
    "commonjs": true,
    "jquery": true
  },
  "rules": {
    "complexity": ["error", { "max": 20 }],
    "comma-dangle": ["error", "always-multiline"],
    "vue/html-indent": ["error", 4],
    "vue/script-indent": ["error", 4, {
      "baseIndent": 1,
      "switchCase": 1
    }]
  },
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {
        "indent": "off"
      }
    }
  ],
  "globals": {
  }
}

